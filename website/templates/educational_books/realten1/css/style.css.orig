@font-face {
  font-family: 'negotiate_free';
  src: url('../fonts/negotiate_free.ttf');
}
@font-face {
  font-family: 'Enriqueta-Regular';
  src: url('../fonts/Enriqueta-Regular.otf');
}
.mailSentMsg {
  font: 1.2em/2.5em QumpellkaNo12;
  border: 1px solid #d2cebe;
  padding: 10px;
}
.sidebarModule .panel-default {
  border: none;
}
.sidebarModule .panel-default .panel-body .btn-info {
  background-color: #5d4a00;
  border-color: transparent;
}
.sidebarModule .panel-default .panel-body .btn-info:hover,
.sidebarModule .panel-default .panel-body .btn-info:active {
  color: #ffffff;
  background-color: #ccb129;
  padding: 7px;
}
.sidebarModule.sidemenu {
  padding: 15px 10px 0px;
}
.sidebarModule.sidemenu ul {
  padding: 0;
  list-style: none;
}
.sidebarModule.sidemenu ul li {
  margin-bottom: 10px;
}
.sidebarModule.sidemenu a {
  display: inline-block;
  height: 90px;
  width: 186px;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  text-indent: -9999px;
  overflow: hidden;
}
.sidebarModule.sidemenu a.area_converter {
  background-image: url('../img/area-converter.png');
}
.sidebarModule.sidemenu a.emi_calculator {
  background-image: url('../img/emi-calculator.png');
}
.sidebarModule.sidemenu a.post_your_property {
  background-image: url('../img/post-your-property.jpg');
}
.sidebarModule.sidemenu a.post_your_requirement {
  background-image: url('../img/post-your-requirement.jpg');
}
.sidebar .nav-pills {
  background: linear-gradient(#171717, #edcf31);
}
.sidebar .nav-pills > li {
  border-right: 1px solid #ccb129;
}
.sidebar .nav-pills > li > a,
.sidebar .nav-pills > li > a:focus {
  color: #d2cebe;
  font: bold 15px/1em Arial, Helvetica, sans-serif;
  position: relative;
  text-transform: uppercase;
  border: 1px solid transparent;
}
.sidebar .nav-pills > li.active > a,
.sidebar .nav-pills > li.active:hover > a,
.sidebar .nav-pills > li.active > a:focus,
.sidebar .nav-pills > li:hover > a,
.sidebar .nav-pills > li:hover > a:focus,
.sidebar .nav-pills > li > a:hover,
.sidebar .nav-pills > li > a:focus {
  color: #ffffff;
  background: linear-gradient(#f9f4da, #5d4a00);
}
.about .table tbody tr th,
.contact .table tbody tr th,
.about .table tbody tr td,
.contact .table tbody tr td {
  border-color: #5d4a00;
}
.about .table {
  margin-top: 20px;
}
.infrastructure .thumbnail {
  background: -webkit-gradient(linear, left top, right top, from(#000000), to(#171717));
  background-image: -webkit-linear-gradient(left, #171717 0%, #000000 5%);
  height: 395px;
}
.infrastructure .thumbnail img {
  max-height: 190px;
}
.infrastructure .thumbnail .caption {
  height: 138px;
  color: #d2cebe;
  margin-bottom: 15px;
}
.infrastructure .thumbnail .readmore .btn-info:hover {
  padding: 6px 40px;
}
.contact {
  background-image: -webkit-linear-gradient(left, #171717 0%, #000000 5%);
  padding: 15px;
}
.contact img {
  max-height: 125px;
}
.contact h4 {
  margin-top: 0px;
}
#googleMap {
  height: 395px;
}
.testimonial {
  background-color: #000000;
  padding: 10px;
}
.testimonial blockquote {
  font-size: inherit;
  border-left: none;
  padding-left: 15px;
}
.testimonial blockquote footer {
  background: none;
  padding: 0px;
}
.testimonial blockquote footer img {
  max-height: 50px;
  margin-right: 10px;
}
.testimonial blockquote footer cite {
  font-size: 17px;
  color: #edcf31;
}
.testimonial blockquote::before {
  content: '\201c';
  position: absolute;
  display: inline-block;
  top: 5px;
  left: 0;
  line-height: 40px;
  font-family: Times New Roman,Serif;
  font-size: 55px;
}
.gallery {
  padding: 10px;
  background: -webkit-gradient(linear, left top, right top, from(#000000), to(#171717));
  background-image: -webkit-linear-gradient(left, #171717 1%, #000000 20%);
}
.gallery .thumbnail {
  background: -webkit-gradient(linear, left top, right top, from(#000000), to(#171717));
  background-image: -webkit-linear-gradient(left, #171717 0%, #000000 5%);
}
.gallery .thumbnail h4 {
  border: none;
  padding-bottom: 0px;
}
.gallery .thumbnail .description {
